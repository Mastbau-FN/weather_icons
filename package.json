{
  "name": "flutter_weather_icons",
  "version": "0.0.0",
  "description": "Wrapper for weather-icons",
  "main": "index.js",
  "repository": "https://github.com/worldturtlemedia/flutter_weather_icons",
  "author": "WorldTurtleMedia <jordon.dehoog@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "commit" : "git-cz",
    "generate": "node tools/scrape_icons.js -o ./lib/src"
  },
  "devDependencies": {
    "fs-extra": "^8.1.0",
    "x-ray": "^2.3.4",
    "yargs": "^13.3.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/exec": "3.3.3",
    "@semantic-release/git": "7.0.12",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "husky": "2.4.1",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2",
    "semantic-release": "15.13.16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.dart": [
      "flutter format",
      "git add"
    ],
    "tools/**/*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
